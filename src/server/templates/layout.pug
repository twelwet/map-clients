doctype html
html
  head
    meta(charset='utf-8')
    link(rel='stylesheet', href='css/style.css')
    link(rel='stylesheet', href='css/leaflet.css')
    link(rel='stylesheet', href='css/bootstrap.css')
    title= title
  body
    header
      ul
        each item in headerMenu
          li(class='list-inline-item')
            if item[`isActive`]
              a= item[`menuName`]
            else
              a(href=`${item[`link`]}`)= item[`menuName`]
    block main
